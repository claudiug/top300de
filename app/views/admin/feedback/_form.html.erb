<%= form_for @feedback, url: {action: :create}, html: {class: "form-horizontal"} do |f| %>

    <%= render "admin/feedback/error" %>
    <div class="form-group">
      <%= f.label :name, class: 'col-lg-1 control-label' %>
      <div class="col-lg-2">
        <%= f.text_field :name, class: 'form-control' %>
      </div>
    </div>
    <div class="form-group">
      <%= f.label :email, class: 'col-lg-1 control-label' %>
      <div class="col-lg-2">
        <%= f.text_field :email, class: 'form-control' %>
      </div>
    </div>
    <div class="form-group">
      <%= f.label :comment, class: 'col-lg-1 control-label' %>
      <div class="col-lg-2">
        <%= f.text_area :comment, class: 'form-control' %>
      </div>
    </div>
    <div class="form-group">
      <%= f.label :pro, class: 'col-lg-1 control-label' %>
      <div class="col-lg-2">
        <%= f.text_field :pro, class: 'form-control' %>
      </div>
    </div>
    <div class="form-group">
      <%= f.label :contra, class: 'col-lg-1 control-label' %>
      <div class="col-lg-2">
        <%= f.text_field :contra, class: 'form-control' %>
      </div>
    </div>
    >
    <div class="form-group">
      <%= f.label :is_active, class: 'col-lg-1 control-label' %>
      <div class="col-lg-2">
        <%= f.check_box :is_active, class: 'form-control' %>
      </div>
    </div>
    <div class="form-group">
      <div class="col-lg-offset-1 col-lg-10">
        <%= f.submit("Add a new Feedback", class: 'btn btn-primary') %>
      </div>
    </div>
<% end%>